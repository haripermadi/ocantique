<table>
    <thead>
        <th>Name</th>
        <th>Price</th>
        <th>Stock</th>
        <th>Description</th>
    </thead>
    <thead>
        <%product.forEach(data=>{%>
            <tr>
                <td><%=data.name%></td>
                <td><%=format.formatCurrency(data.Price)%></td>
                <td><%=data.stock%></td>
                <td><%=data.description%></td>
            </tr>
        <%})%>
    </thead>
</table>